.gameWindow
  background-color #99FF66
  border-radius 5px
  min-height 500px
.gameAge
  text-align center
  margin-top 10px
  margin-left 5px
.gameOptions
  position absolute
  top 20px
  left 20px
  display inline-block
.endTheGame
  margin-top 30px
  margin-left 5px
#map_container
  display inline-block
  margin-top 20px
  margin-left 180px
  border-radius 5px
  background-color #FFFFFF
  padding 10px
  box-shadow 5px 5px 5px gray
.gamePlayers
  margin 5px
.player
  margin 4px 
  margin-top 6px
  border-radius 5px
  box-shadow 5px 5px 5px gray
